<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Todo</title>
</head>
<body>
    


    <h2><%= dayValue %></h2>


    <!-- <ul>

       <% for(var i= 0; i < myNewTodo.length; i++){ %>
            <li><%= myNewTodo[i].name %></li>
        <% } %>
    </ul> -->

    <% myNewTodo.forEach(function(myNewTodo){%>
        <div>
            <input type="checkbox">
            <p><%= myNewTodo.name %></p>
        </div>
    <% }) %>
   
    <form action="/" method="post">
        <input type="text" name="newTodo" placeholder="New item">
        <button type="submit">Submit</button>
    </form>
</body>
</html>